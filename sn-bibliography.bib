@phdthesis{archie_damage_2018,
	title = {Damage nucleation in {DP}-steels: experimental characterization of the contributing microstructural parameters},
	shorttitle = {Damage nucleation in {DP}-steels},
	url = {http://publications.rwth-aachen.de/record/745818},
	abstract = {Most of the advanced high strength steels possess multi-phase microstructures with an inherent mechanical phase contrast, where different damage mechanisms are activated under deformation. This thesis is concerned with analyzing the damage mechanisms induced in ferritic martensitic DP-steels under monotonic and cyclic deformation modes. SEM diffraction techniques were combined to statistically analyze those mechanisms, towards a better understanding of the role of grain and interphase boundaries with respect to strain localization and damage initiation in DP-steels. The utilized techniques included secondary electron (SE) imaging, electron backscatter diffraction (EBSD), and electron channeling contrast imaging (ECCI). The results were confirmed through quasi in situ deformation experiments. It was shown that damage initiation is highly dependent on the distribution and morphology of the martensitic islands, and it was found that cracks inside of the martensite are remarkably restricted to the prior austenite grain boundaries. This sensitivity of the prior austenite grain boundaries (PAGbs) in lath martensite may be attributed to several aspects: (i)	High Mn enrichment. (ii)	Crystallographic incompatibility across the PAGb. (iii)	Tensile residual stresses within the martensitic laths neighboring the PAGb. The first aspect was assessed by atom probe tomography, while the second aspect was investigated using micro-mechanical cantilever bending experiments. Finally, micro-scale residual stresses were measured using the FIB-assisted ring-core milling technique. All three factors are suggested to assist the nucleation and growth of micro-cracks in martensite, particularly along the prior austenite grain boundaries. Heat treatment modifications were implemented in an attempt to optimize the DP-steel microstructure, making it more tolerant towards deformation. As the applied intercritical annealing temperature is increased, the Mn grain boundary enrichment and the residual stresses in martensite decrease, making the prior austenite grain boundaries more resistant to cracking.},
	language = {en},
	urldate = {2022-03-15},
	school = {RWTH Aachen University},
	author = {Archie, Fady},
	collaborator = {Zaefferer, Stefan and Raabe, Dierk and Münstermann, Sebastian},
	year = {2018},
	doi = {10.18154/RWTH-2018-229075},
	note = {Artwork Size: pages 1 Online-Ressource (160 Seiten) : Illustrationen, Diagramme
Pages: pages 1 Online-Ressource (160 Seiten) : Illustrationen, Diagramme
Publication Title: Dissertation
Volume: RWTH Aachen University},
	keywords = {Steel ; Damage ; Strain ; Microscopy ; Residual Stresses ; Atom Probe Tomography ; EBSD ; ECCI ; Crack ; Dual Phase Steel},
	annote = {SeriesInformation
Dissertation, RWTH Aachen University, 2018; Aachen 1 Online-Ressource (160 Seiten) : Illustrationen, Diagramme (2018). = Dissertation, RWTH Aachen University, 2018Other
Published by Aachen},
}

@article{chu_void_1980-1,
	title = {Void {Nucleation} {Effects} in {Biaxially} {Stretched} {Sheets}},
	volume = {102},
	issn = {0094-4289, 1528-8889},
	url = {https://asmedigitalcollection.asme.org/materialstechnology/article/102/3/249/394607/Void-Nucleation-Effects-in-Biaxially-Stretched},
	doi = {10.1115/1.3224807},
	abstract = {The effects of void nucleation occurring during the deformation history on forming limit curves are considered for both in-plane and punch stretching employing a constitutive model of a porous plastic solid. Both plastic strain controlled and stress controlled nucleation processes are simulated by a two parameter void nucleation criterion. For in-plane stretching, plastic strain controlled nucleation can have, in certain circumstances, a significantly destabilizing effect on the forming limit curve. However, within the framework of plane stress theory which neglects the enhancement of the hydrostatic stress due to necking, a stress controlled nucleation process is not found to be significantly destabilizing. In punch stretching a ductile rupture criterion, which limits the maximum volume fraction of voids, as well as the appearance of a well defined thickness trough, is adopted as a localized necking criterion. Only plastic strain controlled void nucleation is considered here in out-of-plane stretching. The resulting forming limit curves have the same shape as those obtained previously with void nucleation neglected.},
	language = {en},
	number = {3},
	urldate = {2022-03-15},
	journal = {Journal of Engineering Materials and Technology},
	author = {Chu, C. C. and Needleman, A.},
	month = jul,
	year = {1980},
	pages = {249--256},
}

@article{andrade_ductile_2011,
	title = {A {Ductile} {Damage} {Nonlocal} {Model} of {Integral}-type at {Finite} {Strains}: {Formulation} and {Numerical} {Issues}},
	volume = {20},
	issn = {1056-7895, 1530-7921},
	shorttitle = {A {Ductile} {Damage} {Nonlocal} {Model} of {Integral}-type at {Finite} {Strains}},
	url = {http://journals.sagepub.com/doi/10.1177/1056789510386850},
	doi = {10.1177/1056789510386850},
	abstract = {This contribution is devoted to the formulation and numerical implementation of a ductile damage constitutive model enriched with a thermodynamically consistent nonlocal theory of integral type. In order to describe ductile deformation, the model takes finite strains into account. To model elasticity, a Hencky-like hyperelastic free energy potential coupled with nonlocal damage is adopted. The thermodynamic consistency of the model is ensured by applying the first and second thermodynamical principles in the global form and the dissipation inequality can be re-written in a local form by incorporating a nonlocal residual that accounts for energy exchanges between material points of the nonlocal medium. The thermodynamically consistent nonlocal model is compared with its associated classical formulation (in which nonlocality is merely incorporated by averaging the damage variable without resorting to thermodynamic potentials) where the thermodynamical admissibility of the classical formulation is demonstrated. Within the computational scheme, the nonlocal constitutive initial boundary value problem is discretized over pseudo-time where it is shown that well established numerical integration strategies can be straightforwardly extended to the nonlocal integral formulation. A modified Newton-Raphson solution strategy is adopted to solve the nonlinear complementarity problem and its numerical implementation, regarding the proposed nonlocal constitutive model, is presented in detail. The results of two-dimensional finite element analyses show that the model is able to eliminate the pathological mesh dependence inherently present under the softening regime if the local theory is considered.},
	language = {en},
	number = {4},
	urldate = {2022-03-16},
	journal = {International Journal of Damage Mechanics},
	author = {Andrade, F.X.C. and César de Sá, J.M.A. and Andrade Pires, F.M.},
	month = may,
	year = {2011},
	pages = {515--557},
}

